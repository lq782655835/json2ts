{
  "name": "json2ts-core",
  "version": "0.1.1",
  "description": "json data to typescript interface",
  "bin": {
    "json2ts": "./index.js"
  },
  "main": "index.js",
  "scripts": {
    "github": "sh deploy.sh",
    "dev": "node ./index.js --url=http://localhost:8002/view/jobs",
    "test": "jest",
    "format": "prettier --write \"./src/**/*.{js,ts,vue,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && git add ."
    }
  },
  "keywords": [
    "typescript"
  ],
  "author": "springleo",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.3",
    "fs-extra": "^7.0.1",
    "json2ts": "0.0.7",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "prettier": "^1.16.1"
  }
}
